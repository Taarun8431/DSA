class Solution {
    public void moveZeroes(int[] nums) {
    int i=0;
    int j=0;
    int temp=0;
    int n=nums.length-1;
     while(j<=n)
     {
       if(nums[j]!=0)
       {
        temp=nums[j];
        nums[i]=nums[j];
        nums[j]=temp;
        i++;
        j++;
       }
       else
       {
        j++;
       }
     } 
     while(i<=n)
     {
        nums[i]=0;
        i++;
     } 
    }
}
