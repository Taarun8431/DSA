class Solution {
    public int singleNumber(int[] nums) {
        Arrays.sort(nums);
        int n=nums.length-1;
        if(nums.length==1)
        {
            return nums[0];
        }
        
        for(int i=1;i<nums.length;i+=2)
        {
            if (nums[i-1]!=nums[i])
            {
             return nums[i-1];
            }
        }
        return nums[n];
 
    }
}       
